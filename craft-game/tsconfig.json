{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@/*": ["*"],
      "@assets/*": ["assets/*"],
      "@components/*": ["components/*"],
      "@hooks/*": ["hooks/*"],
      "@utils/*": ["utils/*"],
      "@typeData/*": ["types/*"],
      "@data/*": ["data/*"],
      "@contexts/*": ["contexts/*"]
    },
    "jsx": "react-jsx",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "target": "es2020",
    "lib": ["dom", "es2020"],
  },
  "include": ["src"]
}
